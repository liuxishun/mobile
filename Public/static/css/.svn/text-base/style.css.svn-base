.spfl-inpu{border:2px solid #d0d0d0;width:80%;float:left;height:50px;margin-bottom:20px;margin-top:20px;}
.spfl-but{width:19%;float:left;background:url(../images/search-but.png) no-repeat  center #c13528;border:none;height:54px;margin-top:20px;}
.spfl-cont{width:92%;margin:0 auto;border:1px solid #dbdbdb;overflow:hidden;background:#fff;}
.spfl-cont ul li{padding:4% 3% 4% 3%;width:94%;float:left;border-bottom:1px solid #dbdbdb;color:#555555;}
.spfl-cont ul li img{float:left;width:2%;margin-top:3.5%;}
.spfl-cont ul .spfl-li{padding:2% 3% 0% 3%;}
.spfl-cont ul li a{background:url(../images/sj-tb.png) no-repeat right center;padding-left:3%;width:95%;float:left;font-size:16px;color:#333;}
.spfl-cont ul li .spfl-li-li{width:100%;float:left;padding-bottom:12px;}
.spfl-cont ul li .spfl-li-li span a{color:#c23627;}
.spfl-cont ul li .spfl-li-li ul li{background:none;width:auto;float:left;border:none;padding:5px;color:#b9b9b9}
.spfl-cont ul li .spfl-li-li ul li a{background:none;float:left;display:block;width:auto;padding-left:0;margin-right:5px;color:#565656}


.zhankai{width:100%;float:left;padding-top:2%;padding-bottom:5%;}
.nav-zk-list{border-bottom:1px solid #d2d2d2;width:90%;margin-left:5%;float:left;padding-top:3%;padding-bottom:3%;}
.nav-zk-list .zk-list-cp{float:left;width:60%;}
.nav-zk-list .zk-list-cp img{float:left;width:35%;}
.nav-zk-list .zk-list-cp span{float:left;width:60%;padding-left:5%;font-size:18px;}
.nav-zk-list .zk-list-cp span b{float:left;font-weight:normal;}
.nav-zk-list .zk-list-cp span em{float:left;margin-top:18%;color:#d90000;}
.nav-zk-list .zk-list-jj{float:right;width:29%;margin-top:8%;text-align:right;}
.nav-zk-list .zk-list-jj a{background:#e3e3e3;width:60%;display:block;padding:2%;float:right;border-radius:4px;text-align:center;color:#7a7a7a;font-weight:bold;font-size:20px;}
.nav-zk-list .zongjia{float:right;}
.nav-zk-list .zongjia b{font-weight:normal;}
.nav-zk-list .zongjia em{margin-top:18%;color:#d90000;}
.zk-list-jj{float:left;border:1px solid #ccc;}
.jj02{border:none;}
.zk-list-jj .sl-jj{float:left;}
.zk-list-jj .sl-jj a{float:left;background:url(../images/jj-bg.png) repeat-x;width:25%;height:22px;display:block;text-align:center;line-height:22px;color:#000;font-weight:bold;border-radius:0px;}
.zk-list-jj .sl-jj .sj-jj-ipn{float:left;width:39%;border:none;border-left:1px solid #ccc;border-right:1px solid #ccc;height:26px;text-align:center;line-height:26px;}
.gwc-dz{width:90%;margin-left:5%;float:left;padding-bottom:3%;}
.gwc-dz ul li{width:100%;border-bottom:1px solid #d2d2d2;padding-bottom:3%;padding-top:3%;float:left;}
.gwc-dz ul li a{color:#0066FF;}
.gwc-dz ul li em{float:left;width:17%;}
.gwc-dz ul li .dz-input{float:left;width:82%;border:1px solid #ccc;padding-top:2%;padding-bottom:2%;}


.tjdd-but{width:90%;float:left;background:#c23629;height:42px;line-height:42px;text-align:center;border:none;border-radius:5px;font-size:20px;color:#fff;}
.qr-but{margin-left:5%;}


/*************************************ding dan cheng  gong***********************************/
.ddcg-cont{width:92%;margin:0 auto;}
.ddcg-cont ul{border:1px solid #ccc;width:100%;float:left;background:#fff;}
.ddcg-cont ul li{border-bottom:1px solid #ccc;padding-top:3%;padding-bottom:4%;padding-left:3%;width:97%;float:left;}
.ddcg-cont ul li b{color:#40a509;}
.ddcg-cont ul li em{color:#f11e0e;}
.ddcg-cont ul li span{float:left;width:25%;text-align:left;}
.ddcg-cont ul li p{float:left;width:75%;text-align:left;}
.ddcg-cont ul li a{float:left;margin-right:5%;}

