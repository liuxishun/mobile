.fbxx-cont{width:93.8%;float:left;overflow:hidden;background:#fff;padding:3%;padding-top:0%;font-size:14px;color:#999;}
.fbxx-cont ul li{padding-top:3%;width:100%;float:left;border-bottom:1px solid #ccc;padding-bottom:3%;}
.fbxx-cont ul li span{float:left;width:23%;text-align:right;font-size:14px;color:#999;color:#555;height:34px;line-height:34px;}
.fbxx-cont ul li .gxtd-input{float:left;border:1px solid #ccc;height:30px;width:70%;line-height:34px;font-size:12px;color:#999;}
.fbxx-cont ul li b{border:1px solid #d0d0d0;border-radius:3px;height:35px;line-height:35px;width:64%;float:left;padding-left:3%;padding-right:3%;}
.fbxx-cont ul li b a{background:url(../images/a-xl-bg.png) no-repeat right center;float:left;color:#999;font-weight:normal;width:100%;float:left;}

.fbxx-cont ul li h2{text-align:center;font-size:24px;color:#9a9a9a;width:100%;padding-top:10%;padding-bottom:10%;}
.input-li{width:64%;float:left;}
.input-li img{float:left;height:34px;}
.fbxx-cont ul li p{width:100%;float:left;text-align:right;padding-top:1%;color:#999}
.fbxx-cont ul li .input-bd{width:64%;float:left;}
.fbxx-cont ul li .input-bd p{float:left;text-align:left;line-height:140%;}



